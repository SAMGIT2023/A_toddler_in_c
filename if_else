//checking eligibility to vote
#include <stdio.h>
/*main - evaluates ages and determines whether one
can vote or not*/
int main()
{
int age;
printf("Enter your age: ");
scanf("%d", &age);

if (age >= 18)
{
printf("You are eligible to vote");
}
else
{
printf("You are not eligible to vote");
}
return (0);
}
