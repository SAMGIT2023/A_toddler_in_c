#include <stdio.h>
/*main - checks if the number is a negative positive
or zero*/
int main()
{
    int x;
    printf("Enter a number: ");
    scanf("%d", &x);
    
    if (x<0)
    printf("%d is negative", x);
    else if (x>0)
    printf("%d is positive", x);
    else
    printf("%d is a zero", x);
    
    return (0);
}
